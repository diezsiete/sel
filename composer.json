{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.2",
        "aws/aws-sdk-php": "^3.133",
        "beberlei/doctrineextensions": "^1.2",
        "ercorp/ssrs-sdk": "^0.0.12",
        "fig/link-util": "^1.1",
        "friendsofsymfony/jsrouting-bundle": "^2.3",
        "knplabs/knp-menu-bundle": "^2.2",
        "knplabs/knp-paginator-bundle": "^4.0",
        "knplabs/knp-time-bundle": "^1.9",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "omines/datatables-bundle": "^0.5",
        "oneup/flysystem-bundle": "^3.1",
        "oneup/uploader-bundle": "^2.2",
        "phpoffice/phpspreadsheet": "^1.9",
        "sensio/framework-extra-bundle": "^5.3",
        "setasign/fpdf": "1.8.1",
        "setasign/fpdi": "1.6.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/amazon-mailer": "^4.0",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.1",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/google-mailer": "^4.0",
        "symfony/http-client": "^4.0",
        "symfony/mailer": "^4.0",
        "symfony/messenger": "^4.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^4.0",
        "vich/uploader-bundle": "^1.15"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "deploy":  [
            "Composer\\Config::disableProcessTimeout",
            "ansible-playbook -i ansible/hosts.ini ansible/deploy.yml --ask-vault-pass -e webpack=1"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.0"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "easycorp/easy-log-handler": "^1.0.7",
        "fzaninotto/faker": "^1.8",
        "symfony/debug-bundle": "*",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "*",
        "symfony/var-dumper": "*",
        "symfony/web-server-bundle": "^4.0"
    }
}
