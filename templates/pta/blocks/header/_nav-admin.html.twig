<div class="header-nav-bar z-index-0"
     data-sticky-header-style="{'minResolution': 991}"
     data-sticky-header-style-active="{'background-color': '#38414a'}"
     data-sticky-header-style-deactive="{'background-color': '#38414a'}">
    <div class="container">
        <div class="header-row">
            <div class="header-column">
                <div class="header-row justify-content-start">
                    <div class="header-nav header-nav-stripe justify-content-start header-nav-icons">
                        <div class="header-nav-admin header-nav-main-square header-nav-main-effect-1 header-nav-main-sub-effect-1">
                            <nav class="collapse">
                                {{ knp_menu_render(
                                    'main', {
                                        'currentClass': 'nav-active',
                                        'ancestorClass': 'nav-expanded nav-active',
                                        'branch_class': 'dropdown',
                                        'root': {
                                            'attributes': {
                                                'class': 'nav nav-pills',
                                                'data-sticky-header-style': '{\'minResolution\': 991}',
                                                'data-sticky-header-style-active': '{\'margin-left\': \'0px\'}',
                                                'data-sticky-header-style-deactive': '{\'margin-left\': \'0\'}',
                                            }
                                        },
                                        'list': {
                                            'attributes': {
                                                'class': 'dropdown-menu'
                                            }
                                        },
                                        'link': {
                                            'attributes': {
                                                'class': {
                                                    'getLevel': {
                                                        1: 'dropdown-item dropdown-toggle',
                                                        2: 'dropdown-item'
                                                    }
                                                }
                                            }
                                        }
                                    }, 'custom') }}
                            </nav>
                        </div>
                        {#<button class="btn header-btn-collapse-nav" data-toggle="collapse" data-target=".header-nav-main nav">
                            <i class="fas fa-bars"></i>
                        </button>#}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>