{% block evaluacion_multiple_ordenar_row %}
    {% for child in form.children if not child.rendered %}

        <div class="form-group row">
            <div class="col-sm-2">
                {{ form_widget(child) }}
            </div>
            {{ form_label(child, null, {'label_attr': {'class' : 'col-sm-10'}}) }}
        </div>
        {{ form_errors(child) }}
    {% endfor %}
    {{ form_errors(form) }}
{% endblock %}