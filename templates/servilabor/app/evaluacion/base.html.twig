{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('css/sel/evaluacion/evaluacion.css') }}">
    <link rel="stylesheet" href="{{ asset('css/sel/evaluacion/' ~ evaluacion.slug ~ '.css') }}">
    <style>
        .g-py-10 {
            padding-bottom: 0.71429rem !important;
        }
    </style>
{% endblock %}

{% block title %}
    Evaluacion - {{ evaluacion.nombre }}
{% endblock %}

{% block nav %}
    {{ include('servilabor/app/evaluacion/_nav.html.twig') }}
{% endblock %}

{% block nav_admin %}
{% endblock %}


{% block main %}
    {{ include('default/_flash_messages.html.twig') }}

    {% block body_header %}
    {% endblock %}

    {% block evaluacion_content %}
        <section class="g-py-20 g-pt-60">
            <div id="evaluacion" class="container">
                <div id="wrapper">
                    {% if form is defined %}
                        {{ form_start(form, {attr: {'novalidate': 'novalidate'}}) }}
                    {% endif %}

                    {% block body %}
                        {{ parent() }}
                    {% endblock %}

                    {% block footer %}
                        <!-- Footer -->
                        <footer id="evaluacion-footer" class="g-bg-darkgray-radialgradient-circle g-color-white-opacity-0_8 g-bg-img-hero">
                            <!-- Copyright Footer -->
                            <div class="g-bg-black-opacity-0_1 g-color-white-opacity-0_8 text-center g-py-20">
                                <div class="container">
                                    <div class="row">
                                        <!-- Footer Content -->
                                        <div class="col-lg-12 col-md-6 text-right">
                                            <ul class="list-inline mb-0">
                                                <li class="list-inline-item g-mr-10">
                                                    {{ sel_evaluacion_navegador_prev(navegador, {
                                                        class: "btn btn-lg g-bg-white-opacity-0_1 g-bg-white-opacity-0_2--hover g-color-white-opacity-0_6 rounded"
                                                    }) }}
                                                </li>
                                                <li class="list-inline-item g-mr-10">
                                                    {{ sel_evaluacion_navegador_next(navegador, {
                                                        class: "btn btn-lg g-bg-white-opacity-0_1 g-bg-white-opacity-0_2--hover g-color-white-opacity-0_6 rounded"
                                                    }) }}
                                                </li>
                                            </ul>
                                        </div>
                                        <!-- End Footer Content -->
                                    </div>
                                </div>
                            </div>
                            <!-- End Copyright Footer -->
                        </footer>
                        <!-- End Footer -->
                    {% endblock %}
                    {% if form is defined %}
                        {{ form_end(form) }}
                    {% endif %}
                </div>
            </div>
        </section>
    {% endblock %}


{% endblock %}
