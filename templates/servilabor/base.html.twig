<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Title -->
    <title>{% block title %}Servilabor{% endblock %}</title>

    <!-- Required Meta Tags Always Come First -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <!-- Favicon -->
    <link rel="shortcut icon" href="{{ asset('/build/servilabor/images/favicon.ico') }}">
    <!-- Google Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800">

    {% block stylesheets %}
        {# CSS Global Compulsory
        <link rel="stylesheet" href="{{ asset('vendor/bootstrap/css/bootstrap.min.css') }}"> -->
        <!-- CSS Global Icons
        <link rel="stylesheet" href="{{ asset('vendor/font-awesome/css/all.min.css') }}">
        <link rel="stylesheet" href="{{ asset('vendor/simple-line-icons/css/simple-line-icons.min.css') }}">
        <link rel="stylesheet" href="{{ asset('vendor/icon-etlinefont/style.css') }}">
        <link rel="stylesheet" href="{{ asset('vendor/icon-line-pro/style.css') }}">

        <link rel="stylesheet" href="{{ asset('vendor/animate.css') }}">
        <link rel="stylesheet" href="{{ asset('vendor/fancybox/jquery.fancybox.min.css') }}">
        <link rel="stylesheet" href="{{ asset('vendor/slick-carousel/slick/slick.css') }}">
        <link rel="stylesheet" href="{{ asset('vendor/typedjs/typed.css') }}">

        #}

        {{ encore_entry_link_tags('servilabor') }}


    {% endblock %}
</head>

<body>
<main>
    {% block header %}
        <!-- Header -->
        <header id="js-header" class="u-header u-header--static">
            {% if is_granted('ROLE_PREVIOUS_ADMIN') %}
                <!-- Top Bar -->
                <div class="u-header__section u-header__section--hidden u-header__section--dark g-bg-black g-py-13 impersonate">
                    <div class="container">
                        <div class="row flex-column flex-md-row align-items-center justify-content-lg-end text-uppercase g-color-white g-font-size-11 g-mx-minus-15">
                            <!-- Responsive Toggle Button -->
                            <div class="col-auto g-px-15">
                                <ul class="list-inline g-line-height-1 g-mt-minus-10 g-mx-minus-4 mb-0">
                                    <li class="list-inline-item g-mx-4 g-mt-10">
                                        <p class="g-color-white g-text-underline--none--hover">
                                            <strong>Viendo a</strong> {{ app.token.user.nombreCompleto(true) }}
                                        </p>
                                    </li>
                                    <li class="list-inline-item g-mx-4 g-mt-10">|</li>
                                    <li class="list-inline-item g-mx-4 g-mt-10">
                                        <a href="{{ path('admin_usuarios', {'_switch_user': '_exit'}) }}"
                                           class="g-color-white g-color-secondary--hover g-text-underline--none--hover">
                                            Salir
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- End Top Bar -->
            {% endif %}

            <div class="u-header__section u-header__section--light g-bg-white g-transition-0_3 g-py-10">
                {% block nav %}
                    {{ include('servilabor/blocks/header/_nav.html.twig') }}
                {% endblock %}


                {% block nav_admin %}
                    {% if app.user %}
                        {{ include('servilabor/blocks/header/_nav-admin.html.twig') }}
                    {% endif %}
                {% endblock %}
            </div>
        </header>
    {% endblock %}
    <!-- End Header -->

    {% block main %}

        {% block body_header %}
            <section class="g-bg-gray-light-v5 g-py-20">
                <div class="container">
                    <div class="d-sm-flex text-center">
                        <div class="align-self-center">
                            <h2 class="h3 g-font-weight-300 w-100 g-mb-10 g-mb-0--md">
                                {% block body_header_title %}
                                {% endblock %}
                            </h2>
                        </div>

                        <div class="align-self-center ml-auto">
                            {% block body_header_right %}
                            {% endblock %}
                        </div>
                    </div>
                </div>
            </section>
        {% endblock  %}

        {% block body %}
        {% endblock %}

        {% block footer %}
            {{ include('servilabor/blocks/footer/_footer.html.twig') }}
        {% endblock %}
    {% endblock %}

</main>

<div class="u-outer-spaces-helper"></div>

{% block javascripts %}
    {#
    <script src="{{ asset('vendor/jquery/jquery.min.js') }}"></script>
    <script src="{{ asset('vendor/jquery-migrate/jquery-migrate.min.js') }}"></script>
    <script src="{{ asset('vendor/popper/umd/popper.min.js') }}"></script>
    <script src="{{ asset('vendor/bootstrap/js/bootstrap.min.js') }}"></script>


    <!-- JS Implementing Plugins -->
    <script src="{{ asset('vendor/slick-carousel/slick/slick.js') }}"></script>

    <script src="{{ asset('vendor/fancybox/jquery.fancybox.min.js') }}"></script>
    <script src="{{ asset('vendor/typedjs/typed.min.js') }}"></script>#}

    {# JS Unify
    <script src="{{ asset('js/servilabor/unify/hs.core.js') }}"></script>
    <script src="{{ asset('js/servilabor/unify/components/hs.carousel.js') }}"></script>
    <script src="{{ asset('js/servilabor/unify/components/hs.tabs.js') }}"></script>
    <script src="{{ asset('js/servilabor/unify/components/hs.popup.js') }}"></script>
    <script src="{{ asset('js/servilabor/unify/components/text-animation/hs.text-slideshow.js') }}"></script>
    <script src="{{ asset('js/servilabor/unify/components/hs.go-to.js') }}"></script>
    <script src="{{ asset('js/servilabor/unify/components/hs.dropdown.js') }}"></script> #}

    {{ encore_entry_script_tags('servilabor') }}


{% endblock %}


</body>

</html>