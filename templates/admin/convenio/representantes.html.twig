{% extends 'datatable/base.datatable.html.twig' %}

{% block title %}
    Convenio
{% endblock %}

{% block body_header_title %}
    {{ convenio.nombre }} <small>{{ convenio.codigo }}</small>
{% endblock %}

{% block body_header_right %}
    {{ parent() }}
    <a href="{{ path('admin_convenio_representante_add', {codigo: convenio.codigo}) }}" class="btn btn-primary">
        Asignar nuevo
    </a>
{% endblock %}

{% block body %}
    {{ include('admin/convenio/_tabs.html.twig') }}
    {{ parent() }}
    {{ include('default/_modal-borrar.html.twig') }}
{% endblock %}


